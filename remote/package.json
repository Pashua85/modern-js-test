{
  "name": "remote",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf node_modules ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "node ../scripts/static-server.js dist 8081",
    "new": "modern new",
    "lint": "biome check",
    "prepare": "simple-git-hooks",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@modern-js/runtime": "2.69.4",
    "@module-federation/modern-js": "0.21.6",
    "@mui/material": "^5.15.15",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.0.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@module-federation/sdk": "^0.21.6",
    "@rspack/core": "^1.6.7",
    "@modern-js/app-tools": "2.69.4",
    "@modern-js/tsconfig": "2.69.4",
    "@biomejs/biome": "1.9.4",
    "typescript": "~5.7.3",
    "@types/jest": "~29.2.4",
    "@types/node": "~18.11.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "~18.3.1",
    "lint-staged": "~15.4.0",
    "simple-git-hooks": "^2.11.1",
    "rimraf": "^6.0.1"
  }
}
